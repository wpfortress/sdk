{
    "name": "wpfortress/sdk",
    "description": "PHP SDK for WPFortress API",
    "license": "MIT",
    "authors": [
        {
            "name": "WPFortress",
            "email": "support@wpfortress.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Wpfortress\\Sdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wpfortress\\Sdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "lint": "vendor/bin/phpcs",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
